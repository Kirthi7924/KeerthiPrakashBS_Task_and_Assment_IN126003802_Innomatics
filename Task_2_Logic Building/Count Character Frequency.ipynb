   "source": [
    "f = {}\n",
    "s = input(\"Enter a sentence:\")\n",
    "s = s.lower()\n",
    "for i in s:\n",
    "    if i ==\" \":\n",
    "        continue\n",
    "    elif i not in f:\n",
    "        f[i]=1\n",
    "    elif i in f:\n",
    "        f[i]+=1 \n",
    "print(f)"
