   ],
   "source": [
    "def calculate(d, p):\n",
    "    b = 50\n",
    "    r = 12\n",
    "    f = b + (d * r)\n",
    "    \n",
    "    if p == \"yes\":\n",
    "        f *= 1.25 \n",
    "    return f\n",
    "\n",
    "d = float(input(\"Enter distance: \"))\n",
    "p = input(\"Is it peak hour: \")\n",
    "t = calculate(d, p)\n",
    "print(\"Total Fare = ₹\", t)"
   ]
