    "def check(marks):\n",
    "    t = 0\n",
    "    for i in marks:\n",
    "        t += i\n",
    "    a = t / len(marks)\n",
    "    if a >= 50:\n",
    "        print(\"Pass\")\n",
    "    else:\n",
    "        print(\"Fail\")\n",
    "marks = [60, 45, 70, 55, 40]\n",
    "check(marks)"
