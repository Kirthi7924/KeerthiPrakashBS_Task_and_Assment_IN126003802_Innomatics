    "def check(a):\n",
    "    c = 0\n",
    "    for i in a:\n",
    "        if i == \"P\":\n",
    "            c += 1\n",
    "    p = (c / len(a)) * 100\n",
    "    if p >= 75:\n",
    "        print(\"Eligible\")\n",
    "    else:\n",
    "        print(\"Not Eligible\")\n",
    "a = [\"P\", \"A\", \"P\", \"P\", \"A\", \"P\", \"P\", \"P\"]\n",
    "check(a)"
   ]
