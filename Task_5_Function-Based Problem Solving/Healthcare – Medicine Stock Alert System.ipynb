    "def medicine_alert(count):\n",
    "    if count>=10:\n",
    "        status=\"Stock Sufficient\"\n",
    "    else:\n",
    "        status=\"Low Stock Alert\"\n",
    "    return status\n",
    "count = int(input(\"Enter the Medicine stock:\"))\n",
    "print(\"Medicine stock:\",count)\n",
    "print(\"Status:\",medicine_alert(count))"
