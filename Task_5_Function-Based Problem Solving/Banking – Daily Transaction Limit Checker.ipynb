    "def transaction_check(amount):\n",
    "    if amount <= 50000:\n",
    "        status = \"Approved\"\n",
    "    else:\n",
    "        status = \"Rejected\"\n",
    "    return status\n",
    "amount = int(input(\"Enter amount to withdraw:\"))\n",
    "print(\"Transaction status:\",transaction_check(amount))"
   ]
