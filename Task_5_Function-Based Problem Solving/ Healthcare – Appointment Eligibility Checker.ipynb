    "def eligibility_check(age):\n",
    "    if age>=18:\n",
    "        status = \"Eligible\"\n",
    "    else:\n",
    "        status = \"Not eligible\"\n",
    "    return status\n",
    "age = int(input(\"Enter age:\"))\n",
    "print(\"Patient age:\",age)\n",
    "print(\"Eligibility Status:\",eligibility_check(age))"
